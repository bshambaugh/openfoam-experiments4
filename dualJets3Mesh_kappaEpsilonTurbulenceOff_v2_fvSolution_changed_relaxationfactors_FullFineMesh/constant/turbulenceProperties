FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType RAS;

RAS
{
    // Tested with kEpsilon, realizableKE, kOmega, kOmegaSST, v2f,
    // ShihQuadraticKE, LienCubicKE.
    RASModel        kEpsilon;
//    RASModel        LRR;

    turbulence      on;

    printCoeffs     on;

   kEpsilonCoeffs
   {
      Cmu             0.09;
   //   C1              1.44;
      C1              1.24;
  //    C2              1.92;
      C2              2.05;
      C3              -0.33;
      sigmak          1;
      sigmaEps        1.3;
    }

}


